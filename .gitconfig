[color]
	diff = auto
	status = auto
	branch = auto
[alias]
	st = status
	nb = checkout -b
	up = pull --rebase
	co = checkout
	br = branch
	serve = !git daemon --reuseaddr --verbose --base-path=. --export-all ./.git
[core]
	editor = vim
	pager = "less -RciqMSj5"
	autocrlf = false
	excludesfile = "/Users/cubox/.git-local-ignores"
[push]
	default = tracking
[merge]
	tool = vimdiff
